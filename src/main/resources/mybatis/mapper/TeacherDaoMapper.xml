<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jiuchou.houpu.dao.TeacherDao">
    <select id="query" resultType="com.jiuchou.houpu.entity.Teacher">
        SELECT * from teacher
    </select>

    <select id="queryById" parameterType="java.lang.String" resultType="com.jiuchou.houpu.entity.Teacher">
        select * from teacher where tid=#{tid}
    </select>


</mapper>